{
  "name": "Teamweek Button",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add tasks to Teamweek directly from Github and Trello",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "https://teamweek.com/",
    "https://*.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://b1149d9764ff4a9a7e8698f7a56ed003796d8582.htmlb.in/"],
      "js": ["scripts/content_blank.js"],
      "css": ["styles/global.css"]
    },
    {
      "matches": ["https://*.github.com/*/*/milestones"],
      "js": ["scripts/content_github.js"],
      "css": ["styles/global.css"]
    }
  ],
  "background": {
    "scripts": ["background/oauth_flow.js"],
    "persistent": false
  },
  "web_accessible_resources": [
    "styles/*",
    "fonts/*"
  ],
  "minimum_chrome_version": "35"
}
