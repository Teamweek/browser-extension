<div class="{{=it.css.container}} row p:items-center">
  <div class="{{=it.css.label}} text--color-new-gray row p:items-center">
    <span class="{{=it.css.icon}} icon icon-svg icon-svg--14" data-hook="heading-icon">
    	{{#def.import('icons/status.svg')}}
    </span>
    Status
  </div>

  <div class="{{=it.css.input}} row p:items-center p:cursor-pointer task-form__field-input-container" data-hook="input" tabindex="2">
    <div class="{{=it.css.tag}} row p:items-center" data-hook="input-label"></div>
    <span class="task-form__readonly {{=it.css.readonly}} icon-svg"></span>
  </div>
</div>
